    "@slack/events-api": "^3.0.1",
    "@slack/interactive-messages": "^2.0.2",
    "@slack/rtm-api": "^6.0.0",
    "@slack/types": "^2.2.0",
    "@slack/web-api": "^6.4.0",
    "@types/node": "^16.11.10",
    "body-parser": "^1.19.0",
    "bull": "^4.1.1",
    "class-transformer": "^0.4.0",
