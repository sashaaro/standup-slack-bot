{
  "name": "standup-slack-bot",
  "version": "0.0.1",
  "description": "Standup slack bot",
  "scripts": {
    "test": "jest",
    "watch": "concurrently --kill-others -n tsc,app,angular,ingress \"tsc -w\" \"nodemon dist/src/cli.js dev\" \"cd frontend && npm start\" \"node deploy/dev-ingress.js\"",
    "build": "tsc",
    "start": "npm run build && node dist/src/cli.js server:run",
    "pkg": "pkg -t=node14-linux-x64 . --output=dist/cli"
  },
  "author": "",
  "license": "ISC",
  "bin": "dist/src/cli.js",
  "pkg": {
    "scripts": "dist/src/migration/*.js"
  },
