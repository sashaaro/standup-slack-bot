    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^28.1.2",
    "@types/pino": "^7.0.5",
    "@types/yargs": "^17.0.8",
    "concurrently": "^7.0.0",
    "jest": "^28.1.1",
    "jest-each": "^28.1.1",
    "nodemon": "^2.0.12",
    "typescript": "^4.7.4"
  }
