{
  "name": "standup-slack-bot",
  "version": "0.0.1",
  "description": "Standup slack bot",
  "scripts": {
    "test": "jest",
    "watch": "concurrently --kill-others -n tsc,app,frontend,ingress \"tsc -w\" \"nodemon --inspect=0.0.0.0:9229 --inspect-brk=0.0.0.0:9229 dist/src/cli.js dev\" \"cd frontend && npm start\" \"node deploy/dev-ingress.js\"",
    "build": "tsc",
    "start": "npm run build && node dist/src/cli.js database:create",
    "pkg": "pkg -t=node16-linux-x64 . --output=dist/cli"
  },
  "author": "",
  "license": "ISC",
  "bin": "dist/src/cli.js",
  "pkg": {
    "scripts": "dist/src/migration/*.js"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.2.0",
    "@mikro-orm/core": "^5.2.0",
    "@mikro-orm/migrations": "^5.2.0",
    "@mikro-orm/postgresql": "^5.2.0",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@slack/events-api": "^3.0.1",
    "@slack/interactive-messages": "^2.0.2",
    "@slack/rtm-api": "^6.0.0",
    "@slack/types": "^2.5.0",
    "@slack/web-api": "^6.7.0",
    "@types/node": "^16.11.22",
    "body-parser": "^1.19.0",
    "bull": "^4.8.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "injection-js": "^2.4.0",
    "ioredis": "^4.27.9",
    "lodash.groupby": "^4.6.0",
    "node-cron": "^3.0.0",
    "pg": "^8.7.1",
    "pino": "^8.0.0",
    "pino-http": "^8.0.1",
    "pino-pretty": "^8.0.0",
