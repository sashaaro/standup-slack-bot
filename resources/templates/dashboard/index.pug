extends layout
block dashboard_content
    .ui.vertical.stripe.segment
        .ui.middle.aligned.stackable.grid.container
            .row
                .eight.wide.column
                    each user in users
                        div.rounded.bg-light.border.row
                            div.col
                                img(src= user.profile.image_72).img-thumbnail
                                span= user.profile.real_name + ' (' + user.name + ')'
                            div.col
                                each question in user.questions
                                    div
                                        div= questionList[question.index]
                                        div= question.answer

                .six.wide.right.floated.column
                    h4= 'For 4 october'
