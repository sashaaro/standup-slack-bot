extends layout

block dashboard_content
    div.row
        div.col
            h4= 'For 4 october'

            each user in users
                div.rounded.bg-light.border.row
                    div.col
                        img(src= user.profile.image_72).img-thumbnail
                        span= user.profile.real_name + ' (' + user.name + ')'
                    div.col
                        each question in user.questions
                            div
                                div= questionList[question.index]
                                div= question.answer
